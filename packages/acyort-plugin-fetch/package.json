{
  "name": "acyort-plugin-fetch-issues",
  "description": "acyort plugin for fetch github issues",
  "version": "1.7.0",
  "dependencies": {
    "@octokit/rest": "^16.13.1",
    "fs-extra": "^7.0.1",
    "log-symbols": "^2.2.0",
    "ora": "^3.0.0"
  },
  "devDependencies": {
    "jest": "24.0.0",
    "nock": "^10.0.6",
    "nodemon": "^1.18.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zWingz/acyort-donob-plugins"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "nodemon ./develop/index.js",
    "test": "jest"
  },
  "main": "index.js",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "issues.json"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/test/**",
      "!**/coverage/**",
      "!**/develop/**"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/lib/issues.json"
    ]
  }
}
